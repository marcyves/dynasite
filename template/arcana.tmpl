	</head>
	<body>
		<div id="page-wrapper">

			<!-- Header -->
				<div id="header">
				@@header@@
					<!-- Logo -->
						<h1>@@titre@@</h1>

					<!-- Nav -->
						<nav id="nav">
						@@menu@@	
						</nav>

				</div>

			<!-- Banner -->
				<section id="banner">
					<header>
						@@intro@@
					</header>
				</section>

			<!-- Highlights -->
				<section class="wrapper style1">
					<div class="container">
					@@contenu@@
					</div>
				</section>

			<!-- Footer -->
				<div id="footer">
					<div class="container">
						<div class="row">
							<section class="3u 6u(narrower) 12u$(mobilep)">
								@@footer@@
							</section>
							<section class="6u 12u(narrower)">
								<h3>Get In Touch</h3>
								@@contact@@
							</section>
						</div>
					</div>

					<!-- Icons -->
					@@social@@
					<!-- Copyright -->
						<div class="copyright">
							<ul class="menu">
